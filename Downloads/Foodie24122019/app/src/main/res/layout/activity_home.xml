<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DFF4F9"
    tools:context=".HomeActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Meal Planner"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/btnMealPlanner" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Food Scanner"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnScanner" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Recipes"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/btnRecipes" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:layout_marginBottom="61dp"
        android:gravity="center"
        android:text="Groceries Nearby"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGroceries" />

    <Button
        android:id="@+id/btnCheckList"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="61dp"
        android:background="@drawable/grocerieschecklist"
        app:layout_constraintEnd_toStartOf="@+id/btnMealPlanner"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnMealPlanner"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="61dp"
        android:background="@drawable/mealplannerlogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnCheckList"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnScanner"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/foodscannerlogo"
        app:layout_constraintEnd_toStartOf="@+id/btnRecipes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnRecipes"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/recipeslogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnScanner"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnGroceries"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/groceriesnearbylogo"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Groceries Checklist"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCheckList" />

    <SeekBar
        android:id="@+id/seekBar"
        android:max="255"
        android:progress="20"
        android:indeterminate="false"
        android:layout_width="331dp"
        android:layout_height="0dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toTopOf="@+id/btnCheckList"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv" />

    <TextView
        android:id="@+id/tv"
        android:layout_width="331dp"
        android:layout_height="19dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="312dp"
        android:layout_marginRight="312dp"
        android:gravity="left"
        android:text="Brightness"
        app:layout_constraintBottom_toTopOf="@+id/seekBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>